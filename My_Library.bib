	Month = jan,
	Number = {1},
	Pages = {19--29},
	Title = {The {Recovery} of {Grey} {Squirrel} ({Sciurus} carolinensis) {Populations} {After} {Intensive} {Control} {Programmes}},
	Url = {http://dx.doi.org/10.3318/BIOE.2007.107.1.19},
