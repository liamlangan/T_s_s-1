	Keywords = {Carcinus maenas, Carcinus maenas, COMPETITION, competition, European shore crab, European shore crab, green crab, green crab, social hierarchy, social hierarchy, synthetic pheromone, synthetic pheromone},
	Month = feb,
	Number = {2},
	Pages = {357--361},
	Shorttitle = {The cost of conflict},
	Title = {The cost of conflict: agonistic encounters influence responses to chemical signals in the {European} shore crab},
	Url = {http://www.sciencedirect.com/science/article/B6W9W-4V053J6-3/2/cec495f02caffa2ef9212f1c7e16c06a},
	Urldate = {2010-04-15},
