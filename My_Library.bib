	File = {Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/BJE5EU8K/abstract.html:text/html;Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/N5GT6BG7/abstract.html:text/html},
