	Title = {Implementing plant hydraulic architecture within the {LPJ} {Dynamic} {Global} {Vegetation} {Model}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1466-8238.2006.00254.x/abstract},
