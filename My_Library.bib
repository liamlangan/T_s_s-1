	Journal = {New Phytologist},
	Keywords = {drought stress, drought stress, isohydric/anisohydric behaviour, isohydric/anisohydric behaviour, leaf water potential, leaf water potential, plant hydraulics, plant hydraulics, stomatal responses, stomatal responses, water availability, water availability, water transport, water transport, xylem embolism, xylem embolism},
	Language = {en},
