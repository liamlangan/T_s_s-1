	Issn = {1365-2699},
	Journal = {Journal of Biogeography},
	Keywords = {Bayesian statistics, Bayesian statistics, calibration, calibration, data assimilation, data assimilation, forest models, forest models, inverse modelling, inverse modelling, MODEL SELECTION, model selection, parameterization, parameterization, plant functional types, plant functional types, predictive uncertainty, predictive uncertainty, process-based models, process-based models},
	Language = {en},
	Number = {12},
	Pages = {2240--2252},
	Title = {Connecting dynamic vegetation models to data -- an inverse perspective},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2699.2012.02745.x/abstract},
	Urldate = {2014-01-23},
