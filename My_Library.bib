	Issn = {1469-8137},
	Journal = {The New phytologist},
	Keywords = {Amazon rainforest, Amazon rainforest, BIOMASS, biomass, BRAZIL, Brazil, DROUGHT, drought, Droughts, Droughts, Plant Stems, Plant Stems, Rain, Rain, SOIL, Soil, Time Factors, Time Factors, tree mortality, tree mortality, TREES, Trees, Wood, Wood, wood production, wood production},
	Language = {eng},
