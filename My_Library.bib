	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Biological invasion, Biological invasion, ecological niche, ecological niche, PHYSIOLOGY, physiology, range size, range size, tree invasions, tree invasions},
	Language = {en},
	Month = jul,
