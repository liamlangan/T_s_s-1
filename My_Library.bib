	Pages = {3--14},
	Shorttitle = {Growing tall vs growing wide},
	Title = {Growing tall vs growing wide: tree architecture and allometry of {Acacia} karroo in forest, savanna, and arid environments},
	Url = {http://onlinelibrary.wiley.com/doi/10.1034/j.1600-0706.2003.12181.x/full},
