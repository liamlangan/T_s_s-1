@article{hoffmann_tree_2009,
	Author = {Hoffmann, William A. and Adasme, Ryan and Haridasan, M. and T. de Carvalho, Marina and Geiger, Erika L. and Pereira, Mireia A. B. and Gotsch, Sybil G. and Franco, Augusto C.},
