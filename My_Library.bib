@incollection{walter_producers_1986,
	Abstract = {The producers of zonobiome I are the plants of the evergreen tropical rain-forest, which has developed in the course of the earth's history from the earliest terrestrial plant communities. We pointed out in Vol. I that it was probably in the climatic zone of the humid tropics that the first cormophytes, the psilophytes, established themselves on land. The pteridophyte forests of the Lower-Carboniferous were also probably limited to this climatic zone. Subsequently, they were gradually replaced by gymnosperms until finally, in the Tertiary, angiosperm tree species became completely dominant. Evidence for this view of the course of evolution is that extant relicts of the oldest pteridophytes are found only or mainly in tropical rain-forests. These are Psilotum, with very small scalelike leaves on rodlike shoots, and Tmesipteris with larger leaves, which, however, are vertically inserted. These two species are the nearest relatives of the now extinct psilophytes.},
	Author = {Walter, Professor Dr Heinrich and Breckle, Professor Dr Siegmar-W.},
	Booktitle = {Ecological {Systems} of the {Geobiosphere}},
	Copyright = {{\copyright}1986 Springer-Verlag Berlin Heidelberg},
	Isbn = {978-3-662-06814-4 978-3-662-06812-0},
	Keywords = {Ecology},
