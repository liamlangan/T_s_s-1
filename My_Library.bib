	Keywords = {Agriculture, Agriculture, Anthropology, Anthropology, Archaeology, Archaeology, Biogeosciences, Biogeosciences, carbon cycle, carbon cycle, climate change, climate change, deforestation, deforestation, Dynamic global vegetation model (LPJ-DGVM), Dynamic global vegetation model (LPJ-DGVM), HOLOCENE, Holocene, Human land-use, Human land-use, Paleontology, Paleontology},
	Language = {en},
	Month = sep,
	Number = {5},
