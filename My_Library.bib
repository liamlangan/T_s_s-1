	Pages = {1275--1278},
	Pmid = {20522775},
	Title = {The {Incidence} of {Fire} in {Amazonian} {Forests} with {Implications} for {REDD}},
	Url = {http://www.sciencemag.org/content/328/5983/1275},
	Urldate = {2013-10-15},
