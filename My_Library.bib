	Pages = {2213--2255},
	Shorttitle = {The dynamic of annual carbon allocation to wood in {European} forests is consistent with a combined source-sink limitation of growth},
	Title = {The dynamic of annual carbon allocation to wood in {European} forests is consistent with a combined source-sink limitation of growth: implications for modelling},
