	Title = {{EFFECTS} {OF} {FOUR} {DECADES} {OF} {FIRE} {MANIPULATION} {ON} {WOODY} {VEGETATION} {STRUCTURE} {IN} {SAVANNA}},
	Url = {http://www.esajournals.org/doi/abs/10.1890/06-1664},
