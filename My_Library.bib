	Issn = {1466-8238},
	Journal = {Global Ecology and Biogeography},
	Keywords = {Alternate stable states, Alternate stable states, remote sensing, remote sensing, SAVANNA, savanna, tree cover, tree cover, TREE-GRASS COEXISTENCE, tree-grass coexistence},
	Language = {en},
