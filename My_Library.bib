	Keywords = {carbon cycling, carbon cycling, GLOBAL CHANGE, global change, nutrient cycling, nutrient cycling, plant functional traits, plant functional traits, roots, roots, SOIL, Soil, soil structure, soil structure},
	Language = {English},
