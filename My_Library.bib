	Journal = {Forestry},
	Language = {en},
	Month = jan,
	Number = {1},
	Pages = {75--86},
	Title = {Forest soil carbon cycle under elevated {CO}2 -- a case of increased throughput?},
	Url = {http://forestry.oxfordjournals.org/content/82/1/75},
	Urldate = {2013-10-02},
