	Journal = {Ecological Modelling},
	Keywords = {BOREAL FORESTS, CARBON, Carbon, CENTRAL CANADA, DECOMPOSITION, Decomposition, DYNAMICS, FOREST, Forest, LITTER DECOMPOSITION, MANAGEMENT, MODEL, Model, Organic matter, Organic matter, ORGANIC-MATTER TURNOVER, REGIONAL-SCALE, SEQUESTRATION, SOIL, Soil, STORAGE, TEMPERATURE},
	Month = nov,
