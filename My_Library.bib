	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Biogeochemistry, Biogeochemistry, carbon cycle, carbon cycle, eddy covariance, eddy covariance, FLUXNET, FLUXNET, plant traits, plant traits},
	Language = {en},
