	Issn = {1469-8137},
	Journal = {New Phytologist},
	Keywords = {Amazon `die-back', Amazon `die-back', Amazon drought, Amazon drought, CO2 fertilization, CO2 fertilization, dynamic global vegetation models (DGVMs), dynamic global vegetation models (DGVMs), elevated temperatures, elevated temperatures, photosynthesis, photosynthesis, PLANT RESPIRATION, plant respiration},
	Language = {en},
	Month = aug,
	Number = {3},
	Pages = {647--665},
	Title = {Multiple mechanisms of {Amazonian} forest biomass losses in three dynamic global vegetation models under climate change},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1469-8137.2010.03350.x/abstract},
