	Keywords = {Biological sciences, biology, Ecology},
	Language = {en},
	Month = jan,
	Number = {1},
	Pages = {63--67},
	Title = {Global diversity of drought tolerance and grassland climate-change resilience},
	Url = {http://www.nature.com/nclimate/journal/v3/n1/full/nclimate1634.html},
	Urldate = {2015-08-13},
	Volume = {3},
	Year = {2013},
	Bdsk-Url-1 = {http://www.nature.com/nclimate/journal/v3/n1/full/nclimate1634.html},
