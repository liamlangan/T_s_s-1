	Keywords = {adaptive dynamic global vegetation model (aDGVM), adaptive dynamic global vegetation model (aDGVM), C4 expansion, C4 expansion, CO2 starvation hypothesis, CO2 starvation hypothesis, dynamic vegetation model, dynamic vegetation model, fire adaptation, fire adaptation, GRASSLAND, grassland, late Miocene, late Miocene, SAVANNA, savanna},
	Language = {en},
	Month = aug,
	Number = {3},
	Pages = {653--666},
	Title = {Fire and fire-adapted vegetation promoted {C}4 expansion in the late {Miocene}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1469-8137.2012.04202.x/abstract},
