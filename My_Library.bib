@article{collatz_coupled_1992-1,
	Abstract = {{\textless}P{\textgreater}Leaf based models of net photosynthesis (A$_{\textrm{n}}$) and stomatal conductance (g) are often components of whole plant, canopy and regional models of net primary productivity and surface energy balance. Since C$_{\textrm{4}}$ metabolism shows unique responses to environmental conditions and C$_{\textrm{4}}$ species are important agriculturally and ecologically, a realistic and accurate leaf model specific to C$_{\textrm{4}}$ plants is needed. In this paper we develop a simple model for predicting A$_{\textrm{n}}$ and g from leaves of C$_{\textrm{4}}$ plants that is easily parameterised and that predicts many of the important environmental responses. {\textless}P{\textgreater}{\textless}/P{\textgreater}        We derive the leaf model from a simple biochemical-intercellular transport model of C$_{\textrm{4}}$ photosynthesis that includes inorganic carbon fixation by PEP carboxylase, light dependent generation of PEP and RuBP, rubisco reaction kinetics, and the diffusion of inorganic carbon and O$_{\textrm{2}}$ between the bundle sheath and mesophyll. We argue that under most conditions these processes can be described simply as three potentially limiting steps. The leaf photosynthesis model treats A$_{\textrm{n}}$ as first order with respect to either light, CO$_{\textrm{2}}$ or the amount of rubisco present and produces a continuous transition between limitations. The independent variables of the leaf photosynthesis model are leaf temperature (T$_{\textrm{I}}$), intercellular CO$_{\textrm{2}}$ levels and the absorbed quantum flux. {\textless}P{\textgreater}{\textless}/P{\textgreater}        A simple linear model of g in terms of A$_{\textrm{n}}$ and leaf surface CO$_{\textrm{2}}$ level (p$_{\textrm{s}}$) and relative humidity (h$_{\textrm{s}}$) is combined with the photosynthesis model to give leaf photosynthesis as a function of absorbed quantum flux, T$_{\textrm{1}}$ and p$_{\textrm{s}}$ and h$_{\textrm{s}}$ levels. {\textless}P{\textgreater}{\textless}/P{\textgreater}        Gas exchange measurements from corn leaves exposed to varied light, CO$_{\textrm{2}}$ and temperature levels are used to parameterise and test the models. Model parameters are determined by fitting the models to a set of 21 measurements. The behaviour of the models is compared with an independent set of 71 measurements, and the predictions are shown to be highly correlated with the data. {\textless}P{\textgreater}{\textless}/P{\textgreater}        Under most conditions the leaf model can be parameterised simply by determining the level of rubisco in the leaves. The effects of light environment, nutritional status and water stress levels on A$_{\textrm{n}}$ and g can be accounted for by appropriate adjustment of the capacity for rubisco to fix CO$_{\textrm{2}}$. We estimate rubsico capacity from CO$_{\textrm{2}}$ and light saturated photosynthesis although leaf nitrogen content or rubisco assays from leaf extracts could also be used for this purpose.{\textless}/P{\textgreater}},
	Author = {Collatz, GJ and Ribas-Carbo, M and Berry, JA},
	File = {CSIRO Publishing Snaphost:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/DP2GTBJP/Collatz et al. - 1992 - Coupled Photosynthesis-Stomatal Conductance Model .html:text/html},
	Journal = {Functional Plant Biol.},
	Month = jan,
	Number = {5},
	Pages = {519--538},
	Title = {Coupled {Photosynthesis}-{Stomatal} {Conductance} {Model} for {Leaves} of {C}4 {Plants}},
	Url = {http://www.publish.csiro.au/paper/PP9920519},
	Urldate = {2012-07-17},
	Volume = {19},
	Year = {1992},
	Bdsk-Url-1 = {http://www.publish.csiro.au/paper/PP9920519}}

