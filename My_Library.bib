	Journal = {Journal of Biogeography},
	Keywords = {Africa, Africa, bioclimatic envelope models, bioclimatic envelope models, climate change, climate change, DESERTIFICATION, desertification, forest degradation, forest degradation, generalized additive model, generalized additive model, human impact, human impact, species distribution modelling, species distribution modelling},
	Language = {en},
	Month = dec,
	Number = {12},
	Pages = {2248--2258},
	Title = {Modelling biome shifts and tree cover change for 2050 in {West} {Africa}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2699.2011.02560.x/abstract},
