	Keywords = {allometry, allometry, biomechanics, biomechanics, carbon construction costs, carbon construction costs, critical buckling height, critical buckling height, tissue-density, tissue-density, wood stiffness, wood stiffness, wood strength, wood strength},
	Language = {en},
	Month = oct,
	Number = {10},
	Pages = {1587--1594},
	Pmid = {21616793},
	Title = {Worldwide correlations of mechanical properties and green wood density},
	Url = {http://www.amjbot.org/content/97/10/1587},
	Urldate = {2014-09-23},
	Volume = {97},
	Year = {2010},
	Bdsk-Url-1 = {http://www.amjbot.org/content/97/10/1587},
