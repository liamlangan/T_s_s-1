	Pages = {519--529},
	Title = {Stoichiometric food quality and herbivore dynamics},
	Url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1461-0248.2001.00240.x/abstract},
	Urldate = {2017-09-27},
	Volume = {4},
