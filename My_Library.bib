	Issn = {1051-0761},
	Journal = {Ecological Applications},
	Month = apr,
	Number = {2},
	Pages = {470--483},
	Title = {{BELOWGROUND} {CONSEQUENCES} {OF} {VEGETATION} {CHANGE} {AND} {THEIR} {TREATMENT} {IN} {MODELS}},
	Url = {http://www.esajournals.org/doi/abs/10.1890/1051-0761(2000)010%5B0470%3ABCOVCA%5D2.0.CO%3B2},
