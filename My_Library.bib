	Number = {1},
	Pages = {233--245},
	Title = {Rates of {Food} {Intake} and {Aggression} of {Oystercatchers} {Haematopus} ostralegus on the {Most} and {Least} {Preferred} {Mussel} {Mytilus} edulis {Beds} of the {Exe} {Estuary}},
