	File = {Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/KC4BZJPA/abstract\;jsessionid=74FD252C88BC9F41969C5382CE90D297.html:text/html},
	Issn = {1469-8137},
	Journal = {New Phytologist},
	Keywords = {AMAZONIAN FORESTS, Amazonian forests, branch, branch, environmental gradients, environmental gradients, neotropical trees, neotropical trees, ROOT, root, wood anatomical traits, wood anatomical traits, WOOD DENSITY, wood density, wood functions, wood functions},
	Language = {en},
	Pages = {n/a--n/a},
	Title = {Wood specific gravity and anatomy of branches and roots in 113 {Amazonian} rainforest tree species across environmental gradients},
