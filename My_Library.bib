	Issn = {1550-7424},
	Journal = {Rangeland Ecology \& Management},
	Keywords = {allometry, allometry, basal diameter, basal diameter, grazing history, grazing history, plant height, plant height, Regression Analysis, Regression Analysis},
	Month = jan,
	Number = {1},
	Pages = {68--72},
	Title = {Multispecies {Allometric} {Models} {Predict} {Grass} {Biomass} in {Semidesert} {Rangeland}},
	Url = {http://www.srmjournals.org/doi/abs/10.2111/08-003},
	Urldate = {2014-09-23},
