	Shorttitle = {The {Distribution} of {Grey} {Squirrel} {Dreys} in {Farm} {Woodland}},
	Title = {The {Distribution} of {Grey} {Squirrel} {Dreys} in {Farm} {Woodland}: {The} {Influence} of {Wood} {Area}, {Isolation} and {Management}},
