@article{otsuka_theoretical_2004,
	Abstract = {For elucidating the mechanisms underlying the ecological pattern formation, ecological systems consisting of autotrophs, predators including omnivorous organisms, and decomposers are theoretically analyzed in terms of several quantities. This analysis reveals that the circular flow of materials driven by organisms is an important measure for the development of ecological systems, although the biomass as well as the numbers of individual types of organisms show variation depending on member organisms. The smaller size of decomposer increasesboth the biomass and the intensity of material cycling, if they decompose thedead bodies of autotrophs at a higher rate than the natural decay constant. However, the increase in these quantities by decomposers is limited in the sense that it is determined by the death rate of autotrophs. On the contrary, predators decrease the biomass in the ecological system but still intensify the material cycling, if the predators capture and digest the prey at a higher rate than the death rate of the latter. These characteristics of the respective types of organisms to intensify the material cycling are significantly observed in the present life world; the larger sizes of plants consisting of cells each covered with a rigid cell wall, active animals with quicker metabolic turnover, and smaller sizes of decomposers such as fungi and eubacteria. Thus, it is proposed that the coexistence of different types of organisms is not a simple result of genome divergence but is directed to intensify the material cycling in a long time scale of evolution. (c) 2004 Elsevier B.V. All rights reserved.},
	Author = {Otsuka, J.},
	Journal = {Ecological Complexity},
	Number = {3},
	Pages = {237--252},
	Title = {A theoretical characterization of ecological systems by circular flow of materials},
	Volume = {1},
	Year = {2004}}

@article{purves_predicting_2008,
	Abstract = {The perfect-plasticity approximation (PPA) is an analytically tractable model of forest dynamics, defined in terms of parameters for individual trees, including allometry, growth, and mortality. We estimated these parameters for the eight most common species on each of four soil types in the US Lake states (Michigan, Wisconsin, and Minnesota) by using short-term (≤15-year) inventory data from individual trees. We implemented 100-year PPA simulations given these parameters and compared these predictions to chronosequences of stand development. Predictions for the timing and magnitude of basal area dynamics and ecological succession on each soil were accurate, and predictions for the diameter distribution of 100-year-old stands were correct in form and slope. For a given species, the PPA provides analytical metrics for early-successional performance (H20, height of a 20-year-old open-grown tree) and late-successional performance ({\^Z}*, equilibrium canopy height in monoculture). These metrics predicted which species were early or late successional on each soil type. Decomposing {\^Z}* showed that (i) succession is driven both by superior understory performance and superior canopy performance of late-successional species, and (ii) performance differences primarily reflect differences in mortality rather than growth. The predicted late-successional dominants matched chronosequences on xeromesic (Quercus rubra) and mesic (codominance by Acer rubrum and Acer saccharum) soil. On hydromesic and hydric soils, the literature reports that the current dominant species in old stands (Thuja occidentalis) is now failing to regenerate. Consistent with this, the PPA predicted that, on these soils, stands are now succeeding to dominance by other late-successional species (e.g., Fraxinus nigra, A. rubrum). climate change community ecology SORTIE TASS trade-offs},
	Author = {Purves, Drew W and Lichstein, Jeremy W and Strigul, Nikolay and Pacala, Stephen W},
	Doi = {10.1073/pnas.0807754105},
	File = {Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/WATEX9TM/17018.html:text/html},
	Issn = {0027-8424, 1091-6490},
	Journal = {PNAS},
	Keywords = {climate change, climate change, COMMUNITY ECOLOGY, community ecology, SORTIE, SORTIE, TASS, TASS, TRADE-OFFS, trade-offs},
	Language = {en},
	Month = nov,
	Number = {44},
	Pages = {17018--17022},
	Title = {Predicting and {Understanding} {Forest} {Dynamics} {Using} a {Simple} {Tractable} {Model}},
	Url = {http://www.pnas.org/content/105/44/17018},
	Urldate = {2012-03-15},
	Volume = {105},
	Year = {2008},
