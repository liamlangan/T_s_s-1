@incollection{walter_orobiome_1986,
	Abstract = {The most striking feature of the orobiome compared with the rest of the zonobiome is that increasing rainfall with altitude results in a diminution of the drought effect with increasing elevation. In India, for example, at the meteorological station at Bombay (11 m NN) a drought period of 7 months and a mean annual rainfall of 1833 mm are recorded, while in the neighbouring mountains, at 810 m NN, annual rainfall is 5178 mm; at 1380 m NN it is as much as 6640 mm, with 3000 mm falling in July alone; here the drought period is reduced to only 6 months. The effect of the dry season is usually also lessened by lower temperatures and longer periods of cloud cover, so that in the montane belt the rain-green forests of the lowlands are replaced by evergreen forests resembling those of orobiome I.},
	Author = {Walter, Professor Dr Heinrich and Breckle, Professor Dr Siegmar-W.},
	Booktitle = {Ecological {Systems} of the {Geobiosphere}},
