	Issn = {1365-2745},
	Journal = {Journal of Ecology},
	Keywords = {fire-prone ecosystems, flammability dimensions, plant flammability strategies, scale},
	Language = {en},
	Month = mar,
	Number = {2},
	Pages = {289--297},
	Title = {Flammability as an ecological and evolutionary driver},
