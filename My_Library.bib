@article{rinaldi_destabilizing_2004,
	Abstract = {The problem of (de)stabilization of ecosystems is discussed in this paper by focusing on two basic questions: (i) Is it possible to derive from a mathematical model exact measures of the (de)stabilizing power of a parameter?; (ii) Are these measures robust with respect to perturbations of the environmental and demographic parameters? After recalling that (de)stabilization can refer to various characteristics of ecosystems, like stability (in the strict sense), resilience, complexity, and risk of extinction, we show that the first question has a positive answer in the special context of slow-fast population assemblies. Then, through the comparative analysis of a few significant cases, we show that there is a convincing evidence that these measures are very strongly influenced by details of the models (or of the real ecosystem). This poses serious doubts on the validity of sharp conclusions on (de)stabilizing factors obtained through simulation and data analysis. (C) 2003 Elsevier B.V. All rights reserved.},
	Author = {Rinaldi, S. and Gragnani, A.},
	Journal = {ECOLOGICAL MODELLING},
	Month = dec,
	Number = {4},
	Pages = {445--460},
	Title = {Destabilizing factors in slow-fast systems},
	Volume = {180},
	Year = {2004}}

@article{osborne_ecological_2009,
	Abstract = {Grasses using the C-4 photosynthetic pathway dominate grasslands and savannahs of warm regions, and account for half of the species in this ecologically and economically important plant family. The C-4 pathway increases the potential for high rates of photosynthesis, particularly at high irradiance, and raises water-use efficiency compared with the C-3 type. It is therefore classically viewed as an adaptation to open, arid conditions. Here, we test this adaptive hypothesis using the comparative method, analysing habitat data for 117 genera of grasses, representing 15 C-4 lineages. The evidence from our three complementary analyses is consistent with the hypothesis that evolutionary selection for C-4 photosynthesis requires open environments, but we find an equal likelihood of C-4 evolutionary origins in mesic, arid and saline habitats. However, once the pathway has arisen, evolutionary transitions into arid habitats occur at higher rates in C-4 than C-3 clades. Extant C-4 genera therefore occupy a wider range of drier habitats than their C-3 counterparts because the C-4 pathway represents a pre-adaptation to arid conditions. Our analyses warn against evolutionary inferences based solely upon the high occurrence of extant C-4 species in dry habitats, and provide a novel interpretation of this classic ecological association.},
	Author = {Osborne, C. P. and Freckleton, R. P.},
	Doi = {10.1098/rspb.2008.1762},
	Journal = {Proceedings of the Rroyal Ssociety Bb-Bbiological sciences},
	Number = {1663},
	Pages = {1753--1760},
	Title = {Ecological selection pressures for {C}₄ photosynthesis in the grasses},
	Volume = {276},
	Year = {2009},
	Bdsk-Url-1 = {http://dx.doi.org/10.1098/rspb.2008.1762}}

@article{warren_evergreen_2004,
	Abstract = {The lifestyles of plants affect their approach to photosynthesis. The long-lived evergreen trees must cope with wide temporal variation in water and nutrient availability that herbaceous species largely avoid. Evergreen trees have thus evolved mechanisms to conserve water and nutrients, and to tolerate water and nutrient stress. However, some of these adaptations have a cost. For example, evergreen trees have lower rates of photosynthesis per unit nitrogen, which can be partly explained by the use of Rubisco for nitrogen storage and the higher internal resistance to CO2 diffusion. The high internal resistance to diffusion of CO2 in evergreen trees might be an unavoidable consequence of anatomical and biochemical traits associated with leaf longevity, nutrient conservation and/or drought tolerance.},
	Author = {Warren, C. R. and Adams, M. A.},
	Journal = {Trends in Plant Science},
