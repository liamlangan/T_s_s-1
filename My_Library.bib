	Issn = {0028-0836},
	Journal = {Nature},
	Keywords = {AREA, DRY MASS, FUNCTIONAL-GROUPS, HIGH-RAINFALL, LEAVES, LIFE-SPAN, NUTRIENT, PHOTOSYNTHESIS-NITROGEN RELATIONS, TREE, USE EFFICIENCY},
