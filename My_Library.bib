	Journal = {New Phytologist},
	Keywords = {carbon starvation, carbon starvation, drought-induced mortality, drought-induced mortality, GLOBAL VEGETATION MODEL, global vegetation model, hydraulic failure, hydraulic failure, nonresprouter, nonresprouter, post-disturbance recovery, post-disturbance recovery, post-drought recovery, post-drought recovery, resprouter, resprouter},
	Language = {en},
	Month = dec,
	Pages = {n/a--n/a},
