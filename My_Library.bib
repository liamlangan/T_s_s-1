	File = {Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/7Q8EU2CN/1615.html:text/html;Snapshot:/Users/laughoutlouder/Library/Application Support/Zotero/Profiles/6fxplo3v.default/zotero/storage/AKSBSNZP/1615.html:text/html},
