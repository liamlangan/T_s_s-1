@article{rankin_assortment_2009,
	Abstract = {Reciprocity is often invoked to explain cooperation. Reciprocity is cognitively demanding, and both direct and indirect reciprocity require that individuals store information about the propensity of their partners to cooperate. By contrast, generalized reciprocity, wherein individuals help on the condition that they received help previously, only relies on whether an individual received help in a previous encounter. Such anonymous information makes generalized reciprocity hard to evolve in a well-mixed population, as the strategy will lose out to pure defectors. Here we analyze a model for the evolution of generalized reciprocity, incorporating assortment of encounters, to investigate the conditions under which it will evolve. We show that, in a well-mixed population, generalized reciprocity cannot evolve. However, incorporating assortment of encounters can favor the evolution of generalized reciprocity in which indiscriminate cooperation and defection are both unstable. We show that generalized reciprocity can evolve under both the prisoner's dilemma and the snowdrift game.},
	Author = {Rankin, Daniel J and Taborsky, Michael},
	Doi = {10.1111/j.1558-5646.2009.00656.x},
	Issn = {1558-5646},
	Journal = {Evolution},
	Keywords = {COOPERATION, Cooperation, direct reciprocity, direct reciprocity, population viscosity, population viscosity, relatedness, relatedness, spatial structure, spatial structure},
	Language = {en},
	Month = jul,
	Number = {7},
	Pages = {1913--1922},
	Title = {Assortment and the {Evolution} of {Generalized} {Reciprocity}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1558-5646.2009.00656.x/abstract},
	Urldate = {2011-12-12},
	Volume = {63},
	Year = {2009},
