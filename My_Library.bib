	Keywords = {biogeochemical dynamics, biogeochemical dynamics, ecosystem dynamics, ecosystem dynamics, ecosystem model, ecosystem model, terrestrial, individual-based model, individual-based model, moment approximation, moment approximation, plant community dynamics, plant community dynamics, scaling, scaling, size- and age-structured, size- and age-structured, South America, South America, sub-grid scale heterogeneity, sub-grid scale heterogeneity, terrestrial, terrestrial biosphere model, terrestrial biosphere model, vegetation dynamics, vegetation dynamics},
	Month = nov,
