	Keywords = {dynamic global vegetation model, dynamic global vegetation model, global carbon cycle, global carbon cycle},
	Language = {en},
	Number = {4},
	Pages = {357--373},
	Shorttitle = {Global response of terrestrial ecosystem structure and function to {CO}2 and climate change},
	Title = {Global response of terrestrial ecosystem structure and function to {CO}2 and climate change: results from six dynamic global vegetation models},
	Url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1365-2486.2001.00383.x/abstract},
