	Shorttitle = {Dynamic {Global} {Vegetation} {Modeling}},
	Title = {Dynamic {Global} {Vegetation} {Modeling}: {Quantifying} {Terrestrial} {Ecosystem} {Responses} to {Large}-{Scale} {Environmental} {Change}},
