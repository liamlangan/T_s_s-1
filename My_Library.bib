	Journal = {Climatic Change},
	Keywords = {Meteorology/Climatology, Meteorology/Climatology},
	Language = {en},
	Month = jul,
	Number = {2-3},
	Pages = {395--412},
	Title = {Deep {Soil} {Moisture} {Storage} and {Transpiration} in {Forests} and {Pastures} of {Seasonally}-{Dry} {Amazonia}},
	Url = {http://link.springer.com/article/10.1023/A%3A1005308930871},
	Urldate = {2013-06-04},
