	Pages = {121--131},
	Shorttitle = {Modelling the population dynamics of the {Mt}. {Graham} red squirrel},
	Title = {Modelling the population dynamics of the {Mt}. {Graham} red squirrel: {Can} we predict its future in a changing environment with multiple threats?},
