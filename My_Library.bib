	Keywords = {Plant ecology, Plant ecology},
	Language = {en},
	Month = nov,
	Number = {7426},
	Pages = {752--755},
	Title = {Global convergence in the vulnerability of forests to drought},
	Url = {http://www.nature.com/nature/journal/v491/n7426/full/nature11688.html},
	Urldate = {2013-01-08},
