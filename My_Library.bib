	Keywords = {aDGVM2, aDGVM2, COEXISTENCE, coexistence, community assembly, community assembly, DGVM, DGVM, dynamic vegetation model, dynamic vegetation model, trait based model, trait based model},
	Language = {en},
	Number = {3},
	Pages = {957--969},
	Shorttitle = {Next-generation dynamic global vegetation models},
	Title = {Next-generation dynamic global vegetation models: learning from community ecology},
