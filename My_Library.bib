	Title = {Future of {African} terrestrial biodiversity and ecosystems under anthropogenic climate change},
	Url = {http://www.nature.com.proxy.ub.uni-frankfurt.de/nclimate/journal/v5/n9/full/nclimate2753.html},
	Urldate = {2015-09-01},
