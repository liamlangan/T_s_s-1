	Language = {en},
	Month = nov,
	Number = {11},
	Pages = {1032--1036},
	Title = {Resilience of {Amazon} forests emerges from plant trait diversity},
	Url = {http://www.nature.com/nclimate/journal/v6/n11/full/nclimate3109.html?WT.feed_name=subjects_forest-ecology},
	Urldate = {2017-03-22},
