	Keywords = {drought responses, drought responses, HYDRAULIC ARCHITECTURE, hydraulic architecture, plant--soil interactions, plant--soil interactions, stomatal regulation, stomatal regulation, WATER RELATIONS, water relations, water transport, water transport, xylem cavitation, xylem cavitation},
	Language = {en},
	Month = feb,
	Number = {2},
	Pages = {251--263},
