	Month = sep,
	Number = {20},
	Pages = {11376--11381},
	Pmcid = {PMC58737},
	Pmid = {11535803},
	Shorttitle = {Phenotypic diversity and ecosystem functioning in changing environments},
	Title = {Phenotypic diversity and ecosystem functioning in changing environments: {A} theoretical framework},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC58737/},
	Urldate = {2012-08-15},
