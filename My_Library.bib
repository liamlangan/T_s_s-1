	Issn = {0304-3800},
	Journal = {Ecological Modelling},
	Keywords = {ALTERNATIVE STABLE STATES, Alternative stable states, DGVM, DGVM, Forest expansion, Forest expansion, SEED DISPERSAL, Seed dispersal, Spatial heterogeneity, Spatial heterogeneity, Subtropical grasslands, Subtropical grasslands},
	Month = nov,
	Pages = {224--232},
	Title = {Feedbacks between vegetation and disturbance processes promote long-term persistence of forest--grassland mosaics in south {Brazil}},
