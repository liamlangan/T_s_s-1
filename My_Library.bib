	Shorttitle = {Biomass allocation to leaves, stems and roots},
	Title = {Biomass allocation to leaves, stems and roots: meta-analyses of interspecific variation and environmental control},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1469-8137.2011.03952.x/abstract},
