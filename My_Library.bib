	Keywords = {Distribution, Distribution, Fish, Fish, Genetic algorithms, Genetic algorithms, Movement, Movement, Neural networks, Neural networks, Spatial, Spatial},
	Month = aug,
	Number = {1--3},
	Pages = {163--178},
	Shorttitle = {Ecology in {Mare} {Pentium}},
	Title = {Ecology in {Mare} {Pentium}: an individual-based spatio-temporal model for fish with adapted behaviour},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165783698001349},
	Urldate = {2013-02-19},
	Volume = {37},
	Year = {1998},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0165783698001349},
